{
  "name": "vue-drag",
  "version": "0.0.1",
  "description": "A simple, Drag component for Vue.js",
  "main": "dist/vue-drag.min.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.dev.js --open --inline --hot",
    "build:debug": "cross-env NODE_ENV=debug webpack --config build/webpack.dist.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config build/webpack.dist.js",
    "build:examples": "webpack --config build/webpack.examples.js",
    "build:all": "npm run build:debug && npm run build:prod && npm run build:examples"
  },
  "author": "chenxuan",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chenxuan0000/vue-drag"
  },
  "keywords": [
    "vue",
    "vuejs",
    "components",
    "drag"
  ],
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "deep-assign": "^2.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "node-sass": "^4.5.2",
    "sass-loader": "^6.0.6",
    "vue": "^2.5.13",
    "vue-loader": "^11.3.3",
    "vue-router": "^3.0.1",
    "vue-style-loader": "^3.0.0",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.5.6",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-dev-server": "^2.9.7"
  },
  "license": "ISC"
}
